                    <div class="row product-grid-3">
                        {{#each products}}
                        <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                            <div class="product-cart-wrap mb-30">
                                <div class="product-img-action-wrap">
                                    <div class="product-img product-img-zoom">
                                        <a href="/view-singleProduct/{{slug}}">
                                            <img class="default-img" src="{{images.image1}}" alt="">
                                            <img class="hover-img" src="{{images.image2}}" alt="">
                                        </a>
                                    </div>
                                    <div class="product-action-1">
                                        {{!-- <a aria-label="Quick view" class="action-btn hover-up"
                                            data-bs-toggle="modal" data-bs-target="#quickViewModal"> --}}
                                            <i class="fi-rs-search"></i></a>
                                        <a aria-label="Add To Wishlist" class="action-btn hover-up"
                                            onclick="addToWishList('{{_id}}')"><i class="fi-rs-heart"></i></a>
                                        {{!-- <a aria-label="Compare" class="action-btn hover-up"
                                            href="shop-compare.html"><i class="fi-rs-shuffle"></i></a> --}}
                                    </div>
                                    <div class="product-badges product-badges-position product-badges-mrg">
                                        <span class="new">New</span>
                                    </div>
                                </div>
                                <div class="product-content-wrap">
                                    <div class="product-category">
                                        <a href="#">{{...filter}}</a>
                                    </div>
                                    <h2><a href="shop-product-right.html">{{name}}</a></h2>
                                    <div class="rating-result" title="90%">
                                        <span>
                                            {{#if discount}}
                                            <span>{{discount}}%</span>
                                            {{else}}
                                            {{/if}}
                                        </span>
                                    </div>
                                    <div class="product-price">
                                        {{#if (oldprice discountedPrice)}}
                                        <span style="text-decoration: line-through;"
                                            class="text-muted">₹{{price}}</span>
                                        {{/if}}
                                        <br>
                                        <span>₹{{prices price discountedPrice}}</span>
                                    </div>
                                    <div class="product-action-1 show">
                                        <a aria-label="Add To Cart" class="action-btn hover-up"
                                            onclick="addToCart('{{_id}}','{{totalQty}}')"><i class="fi-rs-shopping-bag-add"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}

                    </div>

                    {{!-- pagination --}}
