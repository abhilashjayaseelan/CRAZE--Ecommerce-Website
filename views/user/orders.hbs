<main class="main">
    <div class="page-header breadcrumb-wrap">
        <div class="container">
            <div class="breadcrumb">
                <a href="index.html" rel="nofollow">Home</a>
                <span></span> Shop
                <span></span> Orders
            </div>
        </div>
    </div>
    <section class="mt-50 mb-50">
        <div class="container">
            <div>
                <h4 class="content-title card-title">Orders</h4>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="table-responsive">
                        <table class="table shopping-summery text-center">
                            <thead>
                                <tr class="main-heading">
                                    <th scope="col">Sl No.</th>
                                    <th scope="col">OrderId</th>
                                    <th scope="col">Total Price</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            {{#each orderList}}
                            <tbody>
                                <tr>
                                    <td>
                                        {{index @index}}
                                    </td>

                                    <td>
                                        {{id}}
                                    </td>
                                    {{!--<td class="image product-thumbnail"><img src="/images/{{product._id}}.jpg"
                                            alt="#"></td>
                                    <td class="product-des product-name">
                                        <h5 class="product-name"><a href="shop-product-right.html">{{product.name}}</a>
                                        </h5>
                                        <p class="font-xs">{{product.description}}
                                        </p>
                                    </td> --}}
                                    <td class="price" data-title="Price"><span>â‚¹{{totalPrice}}</span></td>
                                    {{!-- <td class="text-center" data-title="Stock">
                                        <span class="color3 font-weight-bold">{{quantity}}</span>
                                    </td> --}}
                                    <td class="text-right" data-title="Cart">
                                        {{#if (eq status "placed")}}
                                        <span class="badge rounded-pill alert-success">Placed</span>
                                        {{else if (eq status "pending")}}
                                        <span class="badge rounded-pill alert-warning">Pending</span>
                                        {{else if (eq status "recieved")}}
                                        <span class="badge rounded-pill alert-success">Recieved</span>
                                        {{else if (eq status "cancelled")}}
                                        <span class="badge rounded-pill alert-danger">Cancelled</span>
                                        {{else if (eq status "return")}}
                                        <p style="color: red;">Return initiated</p>
                                        {{/if}}
                                    <td class="text-right" data-title="Cart">
                                        <a href="/order-details?orderId={{id}}"
                                            class="btn btn-md rounded font-sm">View</a>
                                    </td>
                                </tr>
                            </tbody>
                            {{/each}}
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>

</main>
