<body>

    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Orders
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div>
                    <h3 class="content-title card-title">Order details</h3>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col">OrderId</th>
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                {{#each orderList}}
                                <tbody>
                                    <tr>

                                        <td>
                                            {{id}}
                                        </td>

                                        <td class="image product-thumbnail"><img src="/images/{{product._id}}.jpg"
                                                alt="#"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a
                                                    href="shop-product-right.html">{{product.name}}</a></h5>
                                            <p class="font-xs">{{product.description}}
                                            </p>
                                        </td>
                                        <td class="price" data-title="Price"><span>â‚¹{{product.price}}</span></td>
                                        <td class="text-center" data-title="Stock">
                                            <span class="color3 font-weight-bold">{{quantity}}</span>
                                        </td>
                                        <td class="text-right" data-title="Cart">
                                            {{#if (eq status "placed")}}
                                            <span class="badge rounded-pill alert-success">Placed</span>
                                            {{else if (eq status "pending")}}
                                            <span class="badge rounded-pill alert-warning">Pending</span>
                                            {{else if (eq status "received")}}
                                            <span class="badge rounded-pill alert-success">Recieved</span>
                                            {{else if (eq status "cancelled")}}
                                            <span class="badge rounded-pill alert-danger">Cancelled</span>
                                            {{/if}}
                                        <td class="text-right" data-title="Cart">
                                            <button class="btn btn-sm" onclick="cancelOrder('{{id}}')">Cancel</button>
                                        </td>
                                    </tr>
                                </tbody>
                                {{/each}}
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    </main>
