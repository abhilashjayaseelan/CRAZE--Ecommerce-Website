    <section class="product-tabs section-padding position-relative wow fadeIn animated">
        <div class="bg-square"></div>
        <div class="container">
            <div class="tab-header">
                <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item" role="presentation">
                        <button class="nav-link active" id="nav-tab-one" data-bs-toggle="tab" data-bs-target="#tab-one"
                            type="button" role="tab" aria-controls="tab-one" aria-selected="true">Featured</button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link" id="nav-tab-two" data-bs-toggle="tab" data-bs-target="#tab-two"
                            type="button" role="tab" aria-controls="tab-two" aria-selected="false">Popular</button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link" id="nav-tab-three" data-bs-toggle="tab" data-bs-target="#tab-three"
                            type="button" role="tab" aria-controls="tab-three" aria-selected="false">New
                            added</button>
                    </li>
                </ul>
                <a href="#" class="view-more d-none d-md-flex">View More<i
                        class="fi-rs-angle-double-small-right"></i></a>
            </div>
            <!--End nav-tabs-->



            <div class="tab-content wow fadeIn animated" id="myTabContent">
                <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">
                    <div class="row product-grid-4">
                        {{#each products}}
                        <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                            <div class="product-cart-wrap mb-30">
                                <div class="product-img-action-wrap">
                                    <div class="product-img product-img-zoom">
                                        <a href="/view-singleProduct/{{slug}}">
                                            <img class="default-img" src="{{images.image1}}" alt="">
                                            <img class="hover-img" src="{{images.image3}}" alt="">
                                        </a>
                                    </div>
                                    <div class="product-action-1">
                                        {{!-- <a aria-label="Quick view" class="action-btn hover-up" data-bs-toggle="modal"
                                            data-bs-target=""><i class="fi-rs-eye"></i></a> --}}
                                        <a aria-label="Add To Wishlist" class="action-btn hover-up"
                                            onclick="addToWishList('{{_id}}')"><i class="fi-rs-heart"></i></a>
                                        {{!-- <a aria-label="Compare" class="action-btn hover-up" href="#"><i
                                                class="fi-rs-shuffle"></i></a> --}}
                                    </div>
                                    <div class="product-badges product-badges-position product-badges-mrg">
                                        <span class="hot">Hot</span>
                                    </div>
                                </div>
                                <div class="product-content-wrap">
                                    <div class="product-category">
                                        <a href="#">{{brand}}</a>
                                    </div>
                                    <h2><a href="#">{{name}}</a></h2>
                                    <div class="rating-result" title="90%">
                                        <span>
                                            {{#if discount}}
                                            <span>{{discount}}%</span>
                                            {{else}}
                                            {{/if}}
                                        </span>
                                    </div>
                                    <div class="product-price">
                                        {{#if (oldprice discountedPrice)}}
                                        <span  style="text-decoration: line-through;" class="text-muted">₹{{price}}</span>
                                        {{/if}}
                                        <br>
                                        <span>₹{{prices price discountedPrice}}</span>
                                    </div>
                                    <div class="product-action-1 show">
                                        <a aria-label="Add To Cart" class="action-btn hover-up"
                                            onclick="addToCart('{{_id}}','{{totalQty}}')"><i class="fi-rs-shopping-bag-add"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                    <!--End product-grid-4-->
                </div>




                <section class="banner-2 section-padding pb-0">
